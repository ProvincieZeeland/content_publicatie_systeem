{
  "AzureAd": {
    "Instance": "https://login.microsoftonline.com/",
    "TenantId": "YourTenantId",
    "ClientId": "YourClientId",
    "ClientSecret": "YourClientSecret"
  },
  "DownstreamApi": {
    "BaseUrl": "https://graph.microsoft.com/v1.0",
    "Scopes": ".default"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplicationInsights": {
    "ConnectionString": "YourApplicationInsightsConnectionString"
  },
  "RedirectUri": "YourRedirectUri",
  "GlobalSettings": {
    "StorageTableConnectionstring": "YourStorageTableConnectionstring",
    "JobsStorageTableConnectionstring": "YourJobsStorageTableConnectionstring",
    "FileStorageConnectionstring": "YourFileStorageConnectionstring",

    "ObjectIdentifiersTableName": "YourObjectIdentifiersTableName",
    "SettingsTableName": "YourSettingsTableName",
    "SettingsPartitionKey": "YourSettingsPartitionKey",
    "SettingsRowKey": "YourSettingsRowKey",
    "ToBePublishedTableName": "YourToBePublishedTableName",
    "ToBePublishedPartitionKey": "YourToBePublishedPartitionKey",

    "ContentContainerName": "YourContentContainerName",
    "MetadataContainerName": "YourMetadataContainerName",

    "CallbackUrl": "YourCallbackUrl",
    "CallbackAccessToken": "YourCallbackAccessToken",
    "LoggingLevel": "Debug",

    "RootSiteUrl": "YourRootSiteUrl",
    "TenantId": "YourTenantId",
    "ClientId": "YourClientId",
    "KeyVaultName": "YourKeyVaultName",
    "CertificateName": "YourCertificateName",

    "TermStoreName": "YourTermstoreName",
    "AdditionalObjectId": "OptionalExternalID",
    "PublicDriveIds": [],
    "MetadataMapping": [],
    "ExternalReferencesMapping": [],
    "DropOffMetadataMapping": [],
    "LocationMapping": [],
    "HostName": "YourHostName",

    "WebHookSettings": {
      "EndPoint": "YourWebHookEndPoint",
      "CreateEnabled": "false",
      "ClientState": "YourWebHookClientState",
      "WebHookLists": [
        {
          "SiteId": "YourWebHookSiteId",
          "ListId": "YourWebHookListId",
          "DropOffType": "YourDropOffType"
        }
      ]
    }
